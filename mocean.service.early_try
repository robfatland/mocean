[Unit]
Description=Job to run the Python mocean.py Server code to parse HTTP GET API calls

[Service]
# Nothing to do here?... (Need User=/Group= assignment? Need Type=forking?)
Type=forking
# Does not work:
# ExecStart=/bin/bash -c '/home/ubuntu/anaconda3/bin/python /home/ubuntu/mocean/mocean.py'
ExecStart=/home/ubuntu/anaconda3/bin/python /home/ubuntu/mocean/mocean.py

# This ties the execution to a dependency per the systemd lazy execution model
[Install]
WantedBy=multi-user.target
